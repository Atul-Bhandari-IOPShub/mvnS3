    def GIT_URL = 'git@github.com:Atul-Bhandari-IOPShub/mvnS3.git'
    def branchName = '$Branch'

    // Clone the Git repository
    stage('clone') {
        git url: $GIT_URL , branch: "$BranchName", destinationDir: "$WORKSPACE"
    }

    stage('Maven Build') {
        dir("my-app") {
            sh 'mvn install'
        }
    }
    stage('Maven Deploy') {
        dir("my-app") {
            sh '/mvn deploy'
        }
    }
}

